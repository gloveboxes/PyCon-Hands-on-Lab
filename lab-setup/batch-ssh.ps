$i=1
While ($i -le 9)
    {
        cat ~/.ssh/id_rsa.pub | ssh dev0$i@rpibravo "mkdir -p ~/.ssh; cat >> ~/.ssh/authorized_keys"
        $i
        $i++
    }

$i=10
While ($i -le 25)
    {
        cat ~/.ssh/id_rsa.pub | ssh dev$i@rpibravo "mkdir -p ~/.ssh; cat >> ~/.ssh/authorized_keys"
        $i
        $i++
    }